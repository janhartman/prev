{
        n = 10
        : fib(n)
        where
        var n: int;
        fun fib(n: int): int = {
            if (n < 1)
            then
                ret = 0
            else
                if (n == 1)
                then
                    ret = 1
                else
                    ret = fib(n-1) + fib(n-2)
                end
            end
            : ret
            where
            var ret: int
        }
}